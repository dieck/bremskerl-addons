<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

		<!-- views -->
		<record id="l10n_de_bic_view_res_bank_form" model="ir.ui.view">
			<field name="name">l10n.de.bic.view.res.bank.form</field>
			<field name="model">res.bank</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="base.view_res_bank_form" />
			<field name="arch" type="xml">
				<field name="bic" select="1" position="after">
					<field name="bic_de" select="1" />
				</field>
			</field>
		</record>
		
		<record id="l10n_de_bic_view_res_bank_tree" model="ir.ui.view">
			<field name="name">l10n.de.bic.view.res.bank.tree</field>
			<field name="model">res.bank</field>
			<field name="type">tree</field>
			<field name="inherit_id" ref="base.view_res_bank_tree" />
			<field name="arch" type="xml">
				<field name="country" position="after">
					<field name="bic_de" />
				</field>
			</field>
		</record>

	</data>
</openerp>

<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="action_picking_picker_server" model="ir.actions.server">
            <field name="name">Pick into Picking</field>
            <field name="model_id" ref="model_stock_move"/>
            <field name="state">code</field>
            <field name="code">action = obj.action_picking_picker(context=context)</field>
        </record>
        
        <record id="ir_open_picking_picker_wizard" model="ir.values">
            <field eval="'client_action_multi'" name="key2"/>
            <field eval="'stock.move'" name="model"/>
            <field name="name">Pick into Picking</field>
            <field eval="'ir.actions.server,%d'%action_picking_picker_server" name="value"/>
            <field eval="True" name="object"/>
        </record>

	</data>
</openerp>

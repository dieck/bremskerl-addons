<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

		<record id="view_move_search_reception_outcoming_picking"
			model="ir.ui.view">
			<field name="name">stock.delivery.search.dates.search</field>
			<field name="model">stock.move</field>
			<field name="type">search</field>
			<field name="inherit_id"
				ref="stock.view_move_search_reception_outcoming_picking" />
			<field name="arch" type="xml">
				<xpath expr="//group[@string='Extended Filters...']" position="inside">
					<field name="date" />
					<field name="date_expected" />
				</xpath>
			</field>
		</record>

	</data>
</openerp>
